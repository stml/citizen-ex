{
  "manifest_version": 2,
  "name": "Citizen Ex",
  "version": "0.0",
  "permissions": [
    "tabs",
    "storage",
    "https://freegeoip.net/json/"
  ],
  "background": {
    "scripts": ["lib/URI.js", "lib/lodash.js", "citizen-ex.js"],
    "persistent": false
  },
  "icons": {
    "16": "icon16.png"
  },
  "browser_action": {
    "default_title": "Citizen Ex",
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  }
}
