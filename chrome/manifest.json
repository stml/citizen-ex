{
  "manifest_version": 2,
  "name": "Citizen Ex",
  "version": "0.0",
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "unlimitedStorage",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["lib/URI.js", "lib/lodash.js", "citizen-ex.js"],
    "persistent": false
  },
  "icons": {
    "16": "icon16.png"
  },
  "browser_action": {
    "default_title": "Citizen Ex",
    "default_icon": "icon16.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*","https://*/*"],
      "js": [
        "lib/underscore.js",
        "lib/jquery.js",
        "lib/backbone.js"
      ]
    }
  ]
}
